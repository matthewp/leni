{
  "include": ["src"],
  "compilerOptions": {
    "declaration": true,

    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowJs": true,
    "module": "ES2022",
    "outDir": "./lib",
    "target": "ES2022",
    "lib": [
      "es2022",
      "webworker"
    ]
  }
}